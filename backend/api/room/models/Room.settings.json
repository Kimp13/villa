{
  "kind": "collectionType",
  "collectionName": "rooms",
  "info": {
    "name": "room"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "priceInfo": {
      "type": "json"
    },
    "isUtility": {
      "type": "boolean",
      "default": false
    },
    "header": {
      "type": "string",
      "maxLength": 128,
      "private": false
    }
  }
}
